	<action name="update_hotspot_url">
        <if cond="userdata.language == 'en'">
            <!-- 设置英文热点的 URL -->
            <set hotspots.crm_spot.url="images/hotspots/en/CRM.png" />
        </if>
        <else>
            <!-- 设置中文热点的 URL -->
            <set hotspots.crm_spot.url="images/hotspots/cn/CRM电话间.png" />
        </else>
    </action>
	
	<set var="language" val="en"/>
	<action name="setlang" scope="local"  args="lang">
		trace(lang);
		set(language,get(lang));
		trace(language);
	</action>
	<action name="modifyHotspot">
		trace(language);
		if (language == "cn",
			set(hotspot[spot3].url, images/hotspots/en/CRM.png),
			set(hotspot[spot3].url, images/hotspots/cn/CRM电话间.png));
	</action>

	<!-- startup action - load the first scene -->
	<action name="startup" autorun="onstart">
		if(startscene === null OR !scene[get(startscene)], copy(startscene,scene[0].name); );
		loadscene(get(startscene), null, MERGE);
		if(startactions !== null, startactions() );
	</action>

	<action name="do_crop_animation" scope="local" args="framewidth, frameheight, framerate">
		<!-- define local variables -->
		calc(local.xframes, (caller.imagewidth /framewidth) BOR 0);
		calc(local.frames, xframes * ((caller.imageheight / frameheight) BOR 0));
		def(local.frame, integer, 0);
		
		<!-- set the first frame -->
		calc(caller.crop, '0|0|' + framewidth + '|' + frameheight);
		
		<!-- do the animation -->
		setinterval(calc('crop_anim_' + caller.name), calc(1.0 / framerate),
			if(caller.loaded,
				inc(frame);
				if(frame GE frames, if(caller.onlastframe !== null, callwith(caller, onlastframe() ) ); set(frame,0); );
				mod(xpos, frame, xframes);
				div(ypos, frame, xframes);
				Math.floor(ypos);
				mul(xpos, framewidth);
				mul(ypos, frameheight);
				calc(caller.crop, xpos + '|' + ypos + '|' + framewidth + '|' + frameheight);
			  ,
				<!-- stop the interval when the hotspot gets removed -->
				clearinterval(calc('crop_anim_' + caller.name));
			);
		);
	</action>
